{
     "version": 2,
    "builds":[{
        "src": "licenta/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxlambdaSize": "1Smb", "runtime": "python3.9"}

    },{"src": "build_files.sh",
    "use": "@vercel/static-build",
    "config": {
        "distDir":"staticfiles_build"
    }}],
    "routes":[{
        "src": "/static/(.*)",
        "dest": "licenta/wsgi.py"
    }]
}